{
    "name": "eugenlisov/dm-zoho-crm-wrapper",
    "version": "1.0.13",
    "minimum-stability": "dev",
    "description": "A custom wrapper around the ZCRM PHP SDK V2.**",
    "keywords": ["Zoho","PHP","Zoho CRM"],
    "homepage": "https://github.com/eugenlisov/dm-zoho-crm-wrapper",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DM_ZCRM\\": [
                "app"
              ]
		},
		"classmap": [
			"vendor/zohocrm"
		]
	},
	"autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "zohocrm/php-sdk": "3.0.0"
    },
    "require-dev": {
		"phpunit/phpunit": "10.0.x-dev",
        "symfony/var-dumper": "9999999-dev"
    }
}
